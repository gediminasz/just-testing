{
    "name": "just-testing",
    "displayName": "just-testing",
    "description": "Simple tests runner",
    "version": "0.0.1",
    "publisher": "gediminasz",
    "repository": {
        "type": "git",
        "url": "https://github.com/gediminasz/just-testing"
    },
    "engines": {
        "vscode": "^1.32.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.runOnCursor",
        "onCommand:workbench.action.tasks.runTask"
    ],
    "main": "./src/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "extension.runOnCursor",
                "title": "Run test on cursor"
            }
        ],
        "taskDefinitions": [
            {
                "type": "testAll"
            },
            {
                "type": "testFile"
            }
        ]
    },
    "scripts": {
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^3.3.1",
        "vscode": "^1.1.28",
        "eslint": "^5.13.0",
        "@types/node": "^10.12.21",
        "@types/mocha": "^2.2.42"
    }
}
